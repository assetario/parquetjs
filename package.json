  "version": "0.0.1",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "description": "fully asynchronous, pure JavaScript implementation of the Parquet file format",
  "main": "parquet.js",
